	<queryString>
		<![CDATA[SELECT
     registro_asistencia."fecha" AS registro_asistencia_fecha,
     detalle_registro_asistencia."hora_inicio" AS detalle_registro_asistencia_hora_inicio,
     detalle_registro_asistencia."hora_fin" AS detalle_registro_asistencia_hora_fin,
     Permiso."fecha_inicio" AS Permiso_fecha_inicio,
     Permiso."motivo" AS Permiso_motivo,
     tipo_permiso."tipo_descuento" AS tipo_permiso_tipo_descuento,
     ficha_laboral."codigo_trabajador" AS ficha_laboral_codigo_trabajador,
     persona."paterno" AS persona_paterno,
     persona."materno" AS persona_materno,
     persona."nombres" AS persona_nombres,
     tipo_permiso."nombre" AS tipo_permiso_nombre
FROM
     "dbo"."Permiso" Permiso INNER JOIN "dbo"."detalle_registro_asistencia" detalle_registro_asistencia ON Permiso."id" = detalle_registro_asistencia."permiso_id"
     INNER JOIN "dbo"."tipo_permiso" tipo_permiso ON Permiso."tipo_permiso_codigo" = tipo_permiso."codigo"
     INNER JOIN "dbo"."registro_asistencia" registro_asistencia ON detalle_registro_asistencia."registro_asistencia_id" = registro_asistencia."id"
     INNER JOIN "personal"."persona" persona ON registro_asistencia."empleado_nro_documento" = persona."nro_documento"
     INNER JOIN "personal"."ficha_laboral" ficha_laboral ON persona."nro_documento" = ficha_laboral."persona_nro_documento"
ORDER BY
persona_paterno, persona_materno, persona_nombres, ficha_laboral_codigo_trabajador,Permiso_fecha_inicio,detalle_registro_asistencia_hora_inicio]]>
	</queryString>
